{
  "name": "restapi_hyh",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "prisma migrate deploy && node dist",
    "build": "tsc",
    "redis": "docker run --name redis -e REDIS_PASSWORD=${REDIS_PASS} -p ${REDIS_PORT}:${REDIS_PORT} -d bitnami/redis:latest",
    "db": "docker run --name db -e POSTGRES_PASSWORD=${DATABASE_PASSWORD} -e POSTGRES_USER=${DATABASE_USER} -p ${DATABASE_PORT}:${DATABASE_PORT} -d postgres:alpine",
    "dev": "prisma migrate deploy && concurrently --kill-others 'prisma generate --watch' 'ts-node-dev --respawn --cls --exit-child src'",
    "test": "MODE=test && prisma migrate deploy && jest --passWithNoTests --coverage --runInBand --silent --detectOpenHandles"
  },
